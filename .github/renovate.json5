{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      matchDepTypes: [
        'action',
      ],
      pinDigests: true,
      matchPackageNames: [
        '!conjikidow/bump-version{/,}**',
      ],
    },
  ],
  'pre-commit': {
    enabled: true,
  },
  labels: [
    'automated',
    'dependencies',
  ],
}
