{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
  ],
  packageRules: [
    {
      "matchDepTypes": ["action"],
      "excludePackagePrefixes": ["conjikidow/bump-version"],
      "pinDigests": true,
    },
  ],
  "pre-commit": {
    enabled: true,
  },
  labels: [
    "automated",
    "dependencies",
  ],
}
